{
  "private": false,
  "name": "@hyrioo/vite-plugin-material-symbols-svg",
  "version": "0.1.0",
  "description": "Vite plugin and registry for Material Symbols SVG icons",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./consumer": {
      "types": "./dist/consumer.d.ts",
      "import": "./dist/consumer.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "scripts": {
    "build": "vite build && yarn build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "dev": "vite build --watch"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.3"
  }
}
