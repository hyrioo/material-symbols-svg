{
  "private": false,
  "name": "@hyrioo/vite-plugin-material-symbols-svg",
  "version": "0.1.1",
  "description": "Vite plugin and registry for Material Symbols SVG icons",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyrioo/material-symbols-svg.git",
    "directory": "vite-plugin-material-symbols-svg"
  },
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./consumer": {
      "types": "./dist/consumer.d.ts",
      "import": "./dist/consumer.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build && yarn build:types",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.3"
  }
}
